	<servlet>
		<servlet-name>QuizBuilder</servlet-name>
		<servlet-class>net.gojou.ast.QuizBuilderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QuizBuilder</servlet-name>
		<url-pattern>/quizbuilder</url-pattern>
	</servlet-mapping>

	<servlet>
	    <servlet-name>ElementEntry</servlet-name>
	    <servlet-class>net.gojou.ast.ElementEntryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>ElementEntry</servlet-name>
	    <url-pattern>/elemententry</url-pattern>
	</servlet-mapping>

	<servlet>
	    <servlet-name>ElementUpdate</servlet-name>
	    <servlet-class>net.gojou.ast.ElementCreateUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>ElementUpdate</servlet-name>
	    <url-pattern>/elementupdate</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Quiz</servlet-name>
		<servlet-class>net.gojou.ast.QuizServlet</servlet-class>
		<init-param>
			<param-name>trimSpaces</param-name>
			<param-value>true</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>Quiz</servlet-name>
		<url-pattern>/quiz</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>QuizAnswers</servlet-name>
		<servlet-class>net.gojou.ast.QuizAnswersServlet</servlet-class>
		<init-param>
			<param-name>trimSpaces</param-name>
			<param-value>true</param-value>
		</init-param>

	</servlet>
	<servlet-mapping>
		<servlet-name>QuizAnswers</servlet-name>
		<url-pattern>/quiz/answers</url-pattern>
	</servlet-mapping>



	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
